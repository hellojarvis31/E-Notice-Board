@model IEnumerable<fyp_V1._4.Models.Student>

@{
    ViewBag.Title = "AppliedStudentList";
}

<h2>AppliedStudentList</h2>

<p>
    @Html.ActionLink("Back", "AdminMainMenu")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RollNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DegreeTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Session)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Section)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Semester)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MorningEvening)
        </th>
       <th>
            @Html.DisplayNameFor(model => model.TimeStamp)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RollNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DegreeTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Session)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Section)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Semester)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MorningEvening)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeStamp)
        </td>
        <td>
            @Html.ActionLink("Validate", "ValidateStudent", new { RollNumber = Convert.ToInt32(item.RollNumber), DegreeTitle = item.DegreeTitle.ToString(), Session = item.Session.ToString() }) |
            @Html.ActionLink("Delete", "DeleteFromAppliedTable", new { RollNumber = Convert.ToInt32(item.RollNumber), DegreeTitle = item.DegreeTitle.ToString(), Session = item.Session.ToString() })
            
            
        </td>
    </tr>
}

</table>
