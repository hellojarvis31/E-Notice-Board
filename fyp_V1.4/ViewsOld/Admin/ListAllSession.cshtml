@model IEnumerable<fyp_V1._4.Models.Session>

@{
    ViewBag.Title = "ListAllSession";
}

<h2>ListAllSession</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DegreeTitle)
        </th>
        <th>
            @Html.DisplayName("Session")
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeStamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddingEmployeeId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DegreeTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DegreeSession)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeStamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddingEmployeeId)
        </td>
        <td>
           
           @Html.ActionLink("Delete", "DeleteSession", new {Session = item.DegreeSession.ToString() })
        </td>
    </tr>
}

</table>
